{
  "$defs": {
    "JobType": {
      "enum": [
        "InitializeNova",
        "IngestNewNova",
        "RefreshReferences",
        "DiscoverSpectraProducts",
        "AcquireAndValidateSpectra",
        "IngestPhotometryDataset",
        "NameCheckAndReconcile"
      ],
      "title": "JobType",
      "type": "string"
    }
  },
  "$id": "nova-cat://schemas/events/acquire_and_validate_spectra/1.0.0",
  "additionalProperties": false,
  "properties": {
    "attributes": {
      "additionalProperties": true,
      "title": "Attributes",
      "type": "object"
    },
    "correlation_id": {
      "format": "uuid",
      "title": "Correlation Id",
      "type": "string"
    },
    "dataset_id": {
      "format": "uuid",
      "title": "Dataset Id",
      "type": "string"
    },
    "event_version": {
      "default": "1.0.0",
      "title": "Event Version",
      "type": "string"
    },
    "file_urls": {
      "description": "Upstream URLs to acquire/validate.",
      "items": {
        "type": "string"
      },
      "title": "File Urls",
      "type": "array"
    },
    "initiated_at": {
      "format": "date-time",
      "title": "Initiated At",
      "type": "string"
    },
    "job_type": {
      "$ref": "#/$defs/JobType",
      "default": "AcquireAndValidateSpectra"
    },
    "nova_id": {
      "format": "uuid",
      "title": "Nova Id",
      "type": "string"
    }
  },
  "required": [
    "nova_id",
    "dataset_id"
  ],
  "title": "AcquireAndValidateSpectraEvent",
  "type": "object"
}
